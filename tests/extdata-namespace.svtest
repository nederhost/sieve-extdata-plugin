require "vnd.dovecot.testsuite";
require "vnd.dovecot.extdata";
require "variables";

test_config :set "sieve_extdata_dict_uri" "file:tests/values.dict";
test_config :reload "vnd.dovecot.extdata";

test "Name" {
	if not string :is "${extdata.name}" "Irvin D. Iot" {
		test_fail "failed to retrieve name value: ${extdata.name}";
	}
}

